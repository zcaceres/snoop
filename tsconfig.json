{
  "compilerOptions": {
    "lib": ["dom", "es6"],
    "module": "CommonJS",
    "target": "es6",
    "moduleResolution": "node",
    "noEmit": true,
    "moduleDetection": "force",
    "allowJs": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
